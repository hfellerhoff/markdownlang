{
  "name": "markdownlang",
  "version": "1.0.0",
  "description": "A programming language using markdown syntax",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "markdownlang": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test tests/*.test.js",
    "build": "esbuild src/index.js --bundle --platform=node --format=esm --outfile=mdl --banner:js='#!/usr/bin/env node'"
  },
  "dependencies": {
    "jsep": "^1.3.8",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "esbuild": "^0.27.3"
  }
}
